{
  "name": "wrap-eth-contracts",
  "version": "1.0.0",
  "description": "Ethereum Wrap contract",
  "homepage": "",
  "license": "LGPL-3.0-or-later",
  "main": "index.js",
  "files": [
    "contracts",
    "test",
    "build"
